{
  "name": "@stnew/prismic-apollo-link",
  "version": "1.2.1",
  "description": "An ApolloLink to connect to Prismic's GraphQL API",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
